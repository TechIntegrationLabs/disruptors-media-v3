  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "preview": "serve -s build",
    "auto-commit": "node scripts/auto-commit-agent.js commit",
    "auto-commit:watch": "node scripts/auto-commit-agent.js watch",
    "auto-commit:status": "node scripts/auto-commit-agent.js status",
    "commit": "node scripts/auto-commit-agent.js commit",
    "dev:auto": "./scripts/dev-with-auto-commit.sh --auto-commit",
    "dev:safe": "./scripts/dev-with-auto-commit.sh",
    "clients:sync": "node src/data/clients/clients-sync.js",
    "clients:validate": "node src/data/clients/clients-sync.js",
    "clients:backup": "node -e \"require('./src/data/clients/clients-sync.js').fetchFromGoogleSheets().then(console.log)\"",
    "assets:reorganize": "node scripts/reorganize-dmsite-assets.js",
    "assets:reorganize:dry-run": "node scripts/reorganize-dmsite-assets.js --dry-run",
    "assets:correct": "node scripts/correct-asset-organization.js",
    "airtable:migrate": "node scripts/migrate-to-airtable.js",
    "airtable:migrate:blog": "node -e \"require('./scripts/migrate-to-airtable.js').migrateBlogPosts()\"",
    "airtable:migrate:clients": "node -e \"require('./scripts/migrate-to-airtable.js').migrateClients()\"",
    "airtable:test": "node -e \"require('./src/services/airtableService.js').fetchBlogPostsFromAirtable().then(posts => console.log('Found posts:', posts.length))\"",
    "sync:start": "node scripts/blog-sync-scheduler.js start",
    "sync:stop": "pkill -f blog-sync-scheduler",
    "sync:status": "node scripts/blog-sync-scheduler.js status",
    "sync:bidirectional": "node scripts/blog-sync-scheduler.js bidirectional",
    "sync:airtable-to-sheets": "node scripts/blog-sync-scheduler.js airtable-to-sheets",
    "sync:sheets-to-airtable": "node scripts/blog-sync-scheduler.js sheets-to-airtable",
    "sync:logs": "node scripts/blog-sync-scheduler.js logs",
    "sync:clear-logs": "node scripts/blog-sync-scheduler.js clear-logs",
    "figma:start": "node scripts/start-figma-websocket.js",
    "admin:help": "echo 'ðŸŽ¨ Secret Admin Panel Commands:\n- npm run figma:start (Start Figma WebSocket)\n- Triple-click logo to access admin panel'",
    "claude-bridge": "node src/backend/claude-bridge/server.js",
    "claude-bridge:dev": "nodemon src/backend/claude-bridge/server.js",
    "dev:full": "concurrently \"npm start\" \"npm run claude-bridge\"",
    "dev:full:auto": "concurrently \"npm run dev:auto\" \"npm run claude-bridge\"",
    "claude:status": "curl -s http://localhost:3456/health | jq .",
    "claude:test": "curl -f http://localhost:3456/health && echo 'Claude Bridge is running!' || echo 'Claude Bridge is not running. Start with: npm run claude-bridge'",
    "deploy:railway": "railway deploy",
    "build:railway": "npm run build && npm run claude-bridge:build",
    "claude-bridge:build": "echo 'Claude bridge ready for production'",
    "start:production": "concurrently \"serve -s build -l 3000\" \"npm run claude-bridge\"",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:do": "doctl apps create --spec .do/app.yaml",
    "deploy:all": "npm run deploy:railway && npm run deploy:netlify",
    "env:setup": "echo 'Setting up environment variables for deployment...'",
    "health:check": "curl -f http://localhost:3000 && npm run claude:test",
    "cloudinary:watch": "node scripts/cloudinary-upload-manager.js",
    "cloudinary:process": "node scripts/cloudinary-upload-manager.js --process-existing",
    "cloudinary:tracker": "open image-assets-tracker.csv || start image-assets-tracker.csv",
    "migration:start": "node scripts/cloudinary-migration.js migrate",
    "migration:upload-local": "node scripts/cloudinary-migration.js upload-local",
    "migration:reorganize-dmsite": "node scripts/cloudinary-migration.js reorganize-dmsite",
    "migration:verify": "node scripts/cloudinary-migration.js verify",
    "migration:cleanup": "node scripts/cloudinary-migration.js cleanup",
    "migration:complete": "node scripts/cloudinary-complete-migration.js",
    "migration:complete:dry-run": "node scripts/cloudinary-complete-migration.js --dry-run",
    "assets:analyze": "node scripts/analyze-local-assets.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
